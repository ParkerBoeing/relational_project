<form action="/crags/<%= @crag.id %>" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type="hidden" name="_method" value="PATCH" />
  <p>Edit</p>
    <p>Crag name:</p>
  <input type='text' name='crag[name]'><br/>
    <p>Elevation:</p>
  <input type='integer' name='crag[elevation]'><br/>
    <p>Nearby camping:</p>
  <input type='boolean' name='crag[nearby_camping]'><br/>
  <input type='submit' value="submit">
</form>